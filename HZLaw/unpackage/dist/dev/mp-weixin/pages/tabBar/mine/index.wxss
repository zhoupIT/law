.vip {
	height: 52px;
	margin: 30px 15px 20px 15px;
	padding: 0 15px;
	background-color: #FFFFFF;
	-webkit-box-shadow: 0 2px 8px 0 rgba(0,0,0,0.07);
	        box-shadow: 0 2px 8px 0 rgba(0,0,0,0.07);
	-webkit-border-radius: 6px;
	        border-radius: 6px;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.vip image {
	width: 24px;
	height: 24px;
}
.vip text {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	color: #666666;
	font-size: 14px;
	margin-left: 10px;
}
.vip .activate {
	width: 72px;
	height: 24px;
	line-height: 24px;
	color: #FFFFFF;
	font-size: 12px;
	text-align: center;
	background-image: -webkit-linear-gradient(178deg, #2FA2F8 0%, #2F69F8 93%);
	background-image: linear-gradient(-88deg, #2FA2F8 0%, #2F69F8 93%);
	-webkit-border-radius: 20px;
	        border-radius: 20px;
