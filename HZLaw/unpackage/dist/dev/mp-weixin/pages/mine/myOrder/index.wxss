
.container {
	font-size: 14px;
}
.list {
	padding: 10px 15px;
}
.cell {
	width: 100%;
	height: 138px;
	-webkit-border-radius: 6px;
	        border-radius: 6px;
	background-color: #FFFFFF;
	margin-bottom: 10px;
	padding: 0px 15px;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.cell .head {
	height: 44px;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	border-bottom: .5px #ECECEC solid;
}
.head text:nth-child(1) {
	color: #333333;
	font-size: 14px;
}
.head text:nth-child(2) {
	color: #666666;
	font-size: 12px;
}
.content {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient:  vertical;
	-webkit-box-direction:  normal;
	-webkit-flex-direction:  column;
	        flex-direction:  column;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
}
.name {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	margin-top: 15px;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.name view:nth-child(1) {
	color: #333333;
	font-size: 14px;
}
.name view:nth-child(2) text:nth-child(1) {
	color: #151515;
	font-size: 12px;
}
.name view:nth-child(2) text:nth-child(2) {
	color: #151515;
	font-size: 16px;
}
.expireTime {
	margin-top: 5px;
	font-size: 12px;
	color: #999999;
}
.time {
	margin-top: 5px;
	font-size: 12px;
	color: #999999;
}

